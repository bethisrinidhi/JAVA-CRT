INVERSION COUNT(ARRAYS):

import java.util.*;

class InversionCount{
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int a[]=new int[n];
        int i,j;
        for(i=0;i<n;i++){
            a[i]=s.nextInt();
        }
	int c=0;
	for(i=0;i<n;i++){
	    for(j=i+1;j<n;j++){
		if(j<n){
		   if(a[i]>a[j]){
			c++;
			}
		       }
		     }
		    }
	System.out.println(c);
}
}
