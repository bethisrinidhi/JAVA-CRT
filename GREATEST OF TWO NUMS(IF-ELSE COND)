GREATEST OF TWO NUMS(IF-ELSE):


import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        int a,b;
        Scanner sc=new Scanner(System.in);
        a=sc.nextInt();
        b=sc.nextInt();
        if(a>b){
            System.out.println(a);
        }
        else{
            System.out.println(b);
        }
    }
}
