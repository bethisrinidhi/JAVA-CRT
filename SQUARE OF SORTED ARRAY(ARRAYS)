SQUARE OF SORTED ARRAY(ARRAYS):

import java.util.*;

class SquareOfSortedArr{
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int a[]=new int[n];
	int sq[]=new int[n];
        int i;
        for(i=0;i<n;i++){
            a[i]=s.nextInt();
        }
	for(i=0;i<n;i++){
	   int q=a[i]*a[i];
		sq[i]=q;
		}
	     
	Arrays.sort(sq);
	for(i=0;i<n;i++){
	System.out.print(sq[i]+",");
	}
}
}
