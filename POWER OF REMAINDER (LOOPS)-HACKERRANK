POWER OF REMAINDER (LOOPS)-HACKERRANK:

import java.io.*;
import java.util.*;
import java.lang.*; 

public class Solution {

    public static void main(String[] args) {
     Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int r,d;
            r=n%10;
            n=n/10;
            d=(int)Math.pow(n,r);
            System.out.println(d);
        
    }
}
