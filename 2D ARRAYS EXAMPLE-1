2D ARRAYS EXAMPLE-1:

import java.util.*;
import java.io.*;

class TwoDimen{
public static void main(String args[]){
Scanner sc=new Scanner(System.in);
int a[][]=new int[2][2];
int i,j;
for(i=0;i<2;i++){
	for(j=0;j<2;j++){
		a[i][j]=sc.nextInt();
	}
}
for(i=0;i<2;i++){
	for(j=0;j<2;j++){
		System.out.print(a[i][j]+" ");
	}
System.out.println();
}
}
}
