BUBBLE SORT (ARRAYS):

import java.util.*;

class BubbleSort{
static void bubbleSort(int a[],int n){
for(int i=n-1;i>0;i--){
  for(int j=0;j<i;j++){
       if(a[j]>a[j+1]){
    	   int t=a[j];
	   a[j]=a[j+1];
	   a[j+1]=t;
         }
       }
    }
}
public static void main(String args[]){
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int a[]=new int[n];
int i;
for(i=0;i<n;i++){
a[i]=s.nextInt();
}
bubbleSort(a,n);
for(i=0;i<n;i++){

System.out.println(a[i]+" ");
}
}
}
