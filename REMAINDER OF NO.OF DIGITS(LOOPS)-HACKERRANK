REMAINDER OF NO.OF DIGITS(LOOPS)-HACKERRANK:

import java.io.*;
import java.util.*;
import java.lang.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int d=(int)Math.log10(n)+1;
        int r=n%d;
        System.out.println(r);
        
    }
}
