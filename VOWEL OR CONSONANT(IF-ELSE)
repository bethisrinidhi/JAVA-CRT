VOWEL OR CONSONANTS (IF-ELSE):

import java.util.*;

class VowCons{
public static void main(String args[]){
Scanner sc=new Scanner(System.in);
int a;
a=sc.next().charAt(0);
if((a=='a')||(a=='e')||(a=='i')||(a=='o')||(a=='u')||(a=='A')||(a=='E')||(a=='I')||(a=='O')||(a=='U')){
System.out.println("VOWEL");
}
else{
System.out.println("CONSONANT");
}
}
}
