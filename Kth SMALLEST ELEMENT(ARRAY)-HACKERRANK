Kth SMALLEST ELEMENT(ARRAY)-HACKERRANK:

import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int pos=sc.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++){
            a[i]=sc.nextInt();
            
        }
        Arrays.sort(a);
        System.out.print(a[pos-1]);
    }
    
}
